{
  "daemon": "warwick_heliostat_dome",
  "log_name": "dome@warwick_heliostat",
  "control_machines": ["WarwickHeliostatDome", "WarwickTCS"],
  "azimuth_serial_port": "/dev/dome",
  "azimuth_serial_baud": 115200,
  "azimuth_serial_timeout": 3,
  "park_azimuth": 140,
  "home_azimuth": 120,
  "azimuth_loop_delay": 1,
  "azimuth_move_timeout": 180,
  "shutter_move_timeout": 180,
  "shutter_serial_port": "/dev/ttyUSB0",
  "shutter_serial_baud": 4800,
  "shutter_serial_timeout": 3
}
